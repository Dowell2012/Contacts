create database contacts;

use contacts;

create table person(
cellphone varchar(11) not null,
name varchar(10) not null,
sex varchar(4) not null,
qq varchar(20),
email varchar(20),
groupnum int,
primary key(cellphone),
check(name!='' && cellphone!='')
);

create table fenlei(
id int,
groupname varchar(20),
primary key(id)
);

